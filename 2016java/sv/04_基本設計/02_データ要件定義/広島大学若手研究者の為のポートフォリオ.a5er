[View]
PName=CM_SCHEDULE_INFO_VIEW
LName=スケジュール参照情報
Comment=
ViewOption=
Query=select 	\q5\q|| \q#\q || SUPPORT_KEY		AS PKEY,\n		\q5\q						AS DATA_KBN,\n		SUPPORT_KEY				AS REF_KEY,\n		SUPPORT_START_DATE		AS SCHEDULE_START_DATE,\n		SUPPORT_END_DATE		AS SCHEDULE_END_DATE,\n		SUPPORT_TITLE			AS SCHEDULE_TITLE,\n		SUPPORT_CONTENT			AS SCHEDULE_MEMO,\n		PARTY_CODE				AS PARTY_CODE,\n		URL						AS SCHEDULE_URL,\n		NULL					AS SCHEDULE_TELNO,\n		NULL					AS SCHEDULE_PLACE\nfrom 	SP_SUPPORT_TBL\nwhere	SUPPORT_SPKIKI_KBN = \q1\q\n\nunion all\n\nselect 	\q6\q|| \q#\q ||SUPPORT_KEY		AS PKEY,\n		\q6\q						AS DATA_KBN,\n		SUPPORT_KEY				AS REF_KEY,\n		SUPPORT_START_DATE		AS SCHEDULE_START_DATE,\n		SUPPORT_END_DATE		AS SCHEDULE_END_DATE,\n		SUPPORT_TITLE			AS SCHEDULE_TITLE,\n		SUPPORT_CONTENT			AS SCHEDULE_MEMO,\n		PARTY_CODE				AS PARTY_CODE,\n		URL						AS SCHEDULE_URL,\n		NULL					AS SCHEDULE_TELNO,\n		NULL					AS SCHEDULE_PLACE\nfrom 	SP_SUPPORT_TBL\nwhere	SUPPORT_SPKIKI_KBN = \q2\q\n\nunion all\n\nselect	\q1\q|| \q#\q || EVENT_KEY			AS PKEY,\n		\q1\q						AS DATA_KBN,\n		EVENT_KEY				AS REF_KEY,\n		EVENT_SEND_DATE			AS SCHEDULE_START_DATE,\n		EVENT_START_DATE		AS SCHEDULE_END_DATE,\n		EVENT_TITLE				AS SCHEDULE_TITLE,\n		EVENT_MEMO				AS SCHEDULE_MEMO,\n		PARTY_CODE				AS PARTY_CODE,\n		NULL					AS SCHEDULE_URL,\n        EVENT_TELNO				AS SCHEDULE_TELNO,\n		EVENT_PLACE				AS SCHEDULE_PLACE\nfrom EV_EVENT_TBL\n\nunion all\n\nselect	\q2\q|| \q#\q ||INTERNSHIP_KEY	AS PKEY,\n		\q2\q						AS DATA_KBN,\n		INTERNSHIP_KEY			AS REF_KEY,\n		INTERNSHIP_START_DATE	AS SCHEDULE_START_DATE,\n		INTERNSHIP_END_DATE		AS SCHEDULE_END_DATE,\n		INTERNSHIP_TITLE		AS SCHEDULE_TITLE,\n		INTERNSHIP_MEMO			AS SCHEDULE_MEMO,\n		PARTY_CODE				AS PARTY_CODE,\n		NULL					AS SCHEDULE_URL,\n        INTERNSHIP_TELNO		AS SCHEDULE_TELNO,\n		NULL					AS SCHEDULE_PLACE\nfrom IT_INTERN_TBL
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
Position="MAIN",3700,300,294,339
Page=MAIN
Left=3700
Top=300

[View]
PName=EV_EVENT_VIEW
LName=イベントVIEW
Comment=
ViewOption=
Query=SELECT\n    A.EVENT_KEY\n  , A.EVENT_TITLE\n  , A.EVENT_SEND_DATE\n  , A.EVENT_RECRUIT\n  , A.EVENT_TELNO\n  , A.PARTY_CODE\n  , A.EVENT_START_DATE\n  , A.EVENT_PLACE\n  , A.EVENT_MEMO\n  , A.EVENT_UNIT\n  , A.SUBJECT_INS_KBN\n  , A.PUBLIC_FLAG\n  , A.UPD_DATE\n  , A.UPD_USER_KEY\n  , \q[\q || ARRAY_TO_STRING(ARRAY_AGG(B.ROLE), \q],[\q)  ||  \q]\q AS SEARCH_ROLE\n  , \q[\q || ARRAY_TO_STRING(ARRAY_AGG(C.PARTY_CODE), \q],[\q)  ||  \q]\q AS SEARCH_PARTY_CODE\n  , ARRAY_TO_STRING(ARRAY_AGG(C.PARTY_NAME), \q,\q) AS PARTY_NAME\n  , ARRAY_TO_STRING(ARRAY_AGG(C.PARTY_NAME_EN), \q,\q) AS PARTY_NAME_EN\n FROM\n  EV_EVENT_TBL A \n  INNER JOIN EV_EVENT_PUBLIC_TBL B \n    ON A.EVENT_KEY = B.EVENT_KEY \n  INNER JOIN MS_PARTY_TBL C \n    ON B.PARTY_CODE = C.PARTY_CODE \n GROUP BY\n    A.EVENT_KEY\n  , A.EVENT_TITLE\n  , A.EVENT_SEND_DATE\n  , A.EVENT_RECRUIT\n  , A.EVENT_TELNO\n  , A.PARTY_CODE\n  , A.PARTY_NAME\n  , A.EVENT_START_DATE\n  , A.EVENT_PLACE\n  , A.EVENT_MEMO\n  , A.EVENT_UNIT\n  , A.SUBJECT_INS_KBN\n  , A.PUBLIC_FLAG\n  , A.UPD_DATE\n  , A.UPD_USER_KEY
LNameByComment=1
Color=$000000
BkColor=$FFFFFF
